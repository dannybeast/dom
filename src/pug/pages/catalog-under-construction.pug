block content

  +b.SECTION.catalog
   .container
    +b.H1.page-title(class="__title") Проекты домов

    include ../includes/modules/_catalog-navigation

    +b.page-filter(class="__filter")
     +filter({storey, square, type, region,additionally})
     +e.footer-row
      +page-sorting()
      +filter-links()
    +e.row 
     each item in items
      +e.col 
       +catalog-item(item)
        +e.UL.table-list 
         +e.LI 
          +e.table-list-name Подрядчик:
          +e.table-list-value 
           span.color-primary ТЕХНОКАРКАС
         +e.LI 
          +e.table-list-name Тип конструкции:
          +e.table-list-value 
           span Блочный
         +e.LI 
          +e.table-list-name Фундамент:
          +e.table-list-value 
           span Утепленная шведская плита
         +e.LI 
          +e.table-list-name Тип кровли:
          +e.table-list-value 
           span Двускатная
         +e.LI 
          +e.table-list-name Материал кровли:
          +e.table-list-value 
           span.color-primary SHINGLAS
         +e.LI 
          +e.table-list-name Внешняя отделка:
          +e.table-list-value 
           span Фиброцементный сайдинг Decover
         +e.LI 
          +e.table-list-name Внутренняя отделка:
          +e.table-list-value 
           span ГКЛ
    include ../includes/modules/_pagination

  include ../includes/modules/_section-calculate

  include ../includes/modules/_section-know
  
