block content

  +b.SECTION.catalog
   .container
    +b.H1.page-title(class="__title") Проекты домов

    include ../includes/modules/_catalog-navigation

    +b.page-filter(class="__filter")
     +filter({storey, square, price, type, bedrooms, region, additionally, resetBtn})
     +e.footer-row
      +page-sorting()
      +filter-links()
      
    +e.row 
     each item in items
      +e.col 
       +catalog-item(item)
        +e.UL.table-list 
         +e.LI 
          +e.table-list-name Тип конструкции:
          +e.table-list-value 
           span Блочный
         +e.LI 
          +e.table-list-name Фундамент:
          +e.table-list-value 
           span Утепленная шведская плита
         +e.LI 
          +e.table-list-name Кровля:
          +e.table-list-value 
           span Двускатная, SHINGLAS
         +e.LI 
          +e.table-list-name Внешняя отделка:
          +e.table-list-value 
           span HAUBERK

        +e.price-subtitle Продан за:
        +e.price=item.priceHidden
    include ../includes/modules/_pagination

  include ../includes/modules/_section-calculate

  include ../includes/modules/_section-know
  
